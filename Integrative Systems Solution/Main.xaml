<Page x:Class="Integrative_Systems_Solution.Main"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Integrative_Systems_Solution"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Main">

    <Grid>
        <Button x:Name="Load_Unload" Content="Load/Unload" HorizontalAlignment="Center" VerticalAlignment="Top" Height="100" Width="300" Margin="0,100,0,0" FontFamily="Arial" FontSize="36" Click="Load_Unload_Click"/>
        <Button x:Name="Balance" Content="Balance" HorizontalAlignment="Center" VerticalAlignment="Top" Height="100" Width="300" Margin="0,250,0,0" FontFamily="Arial" FontSize="36" Click="Balance_Click"/>


    </Grid>
</Page>
